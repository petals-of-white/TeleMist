<Window x:Class="TeleMist_v._2._0.testingPicker"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TeleMist_v._2._0"
        xmlns:rur="clr-namespace:TeleMist_v._2._0.UserControls"
        mc:Ignorable="d"
        Title="testingPicker" Height="450" Width="800">
    <Window.Resources>
        
    </Window.Resources>
    <StackPanel>
        <Calendar x:Name="kik"/>
        <TextBox x:Name="kek">11/11/11</TextBox>
        <TextBox x:Name="kok"></TextBox>
        <rur:TimePicker AppointmentDate="{Binding ElementName=kik, Path=SelectedDate}"></rur:TimePicker>
    </StackPanel>
</Window>
