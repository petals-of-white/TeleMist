<Application
    x:Class="TeleMistUI.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:TeleMistUI"
    LoadCompleted="Application_LoadCompleted"
    Startup="Application_Startup"
    StartupUri="Windows/AuthWindow.xaml">

    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Configs/Config.xaml" />
                <ResourceDictionary Source="Styles/WindowsPages.xaml" />
                <ResourceDictionary Source="Styles/OtherStyles.xaml" />
                <ResourceDictionary Source="Styles/Buttons.xaml" />
                <ResourceDictionary Source="Styles/Calendar.xaml" />
                <ResourceDictionary Source="Styles/TextBlocks.xaml" />
                <ResourceDictionary Source="Styles/TextBoxes.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
